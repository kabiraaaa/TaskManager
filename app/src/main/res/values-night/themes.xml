<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyApplication" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
    </style>

    <style name="Widget.App.Chip" parent="Widget.Material3.Chip.Suggestion.Elevated">
        <item name="chipBackgroundColor">#848A94</item>
        <item name="chipStrokeWidth">2dp</item>
        <item name="chipStrokeColor">@color/home_chip_background_color</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="checkedIconVisible">false</item>
        <item name="shapeAppearanceOverlay">@null</item>
    </style>

    <style name="Base.Widget.AppCompat.TextView.Title" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Widget.MaterialComponents.ShapeableImageView.imageView" parent="Widget.MaterialComponents.ShapeableImageView">
        <item name="android:tint">@android:color/white</item>
    </style>

    <color name="black_white">#FFFFFF</color>
    <color name="white_black">#000000</color>

</resources>